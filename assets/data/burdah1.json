[
    {
        "id": 1,
        "arabic": "أَمِنْ تَذَكُّرٍ جِيْرَانٍ بِذِيْ سَلَمٍ",
        "arabic2" : "مَزَجْتَ دَمْعًا جَرَى مِنْ مُقْلَةٍ بِدَمِ"
    },
    {
        "id": 2,
        "arabic": "أَمْ هَبَّتِ الرِّيْحُ مِنْ تِلْقَاءِ كَاظِمَةٍ",
        "arabic2": "وَأَوْمَضَ الْبَرْقُ فِي الظَّلْمَاءِ مِنْ إِضَمِ"
    },
    {
        "id": 3,
        "arabic": "فَمَا لِعَيْنَيْكَ إِنْ قُلْتَ اكْفُفَا هَمَتَا ",
        "arabic2": "وَمَا لِقَلْبِكَ إِنْ قُلْتَ اسْتَفِقْ يَهِمِ"
    },
    {
        "id": 4,
        "arabic": "أَيَحْسَبُ الصَّبُّ أَنَّ الْحُبَّ مُنْكَتِمٌ",
        "arabic2": "مَا بَيْنَ مُنْسَجِمٍ مِنْهُ وَمُضْطَرِمِ"
    },
    {
        "id": 5,
        "arabic": "لَوْلَا الْهَوَى لَمْ تُرِقْ دَمْعًا عَلَى طَلَلٍ",
        "arabic2":"وَلَا أَرِقْتَ لِذِكْرِ الْبَانِ وَالْعَلَمِ"
    },
    {
        "id": 6,
        "arabic": "فَكَيْفَ تُنْكِرُ حُبًّا بَعْدَمَا شَهِدَتْ ",
        "arabic2":"بِهِ عَلَيْكَ عُدُوْلُ الدَّمْعِ وَالسَّقَمِ"
    },
    {
        "id": 7,
        "arabic": "وَأَثْبَتَ الْوَجْدُ خَطَّيْ عَبْرَةٍ وَضَنًى ",
        "arabic2":"مِثْلَ الْبَهَارِ عَلَى خَدَّيْكَ وَالْعَنَمِ"
    },
    {
        "id": 8,
        "arabic": " نَعَمْ سَرَى طَيْفُ مَنْ أَهْوَى فَأَرَّقَنِي ",
        "arabic2":"وَالْحُبُّ يَعْتَرِضُ اللَّذَّاتِ بِالْأَلَمِ"
    },
    {
        "id": 9,
        "arabic": "يَا لَائِمِي فِي الْهَوَى الْعُذْرِيِّ مَعْذِرَةً ",
        "arabic2":"مِنِّي إِلَيْكَ وَلَوْ أَنْصَفْتَ لَمْ تَلُمِ"
    },
    {
        "id": 10,
        "arabic": "عَدَتْكَ حَالِيَ لَا سِرِّي بِمُسْتَتِرٍ",
        "arabic2":"عَنِ الْوُشَاةِ وَلَا دَائِي بِمُنْحَسِمِ"
    },
    {
        "id": 11,
        "arabic": " مَحَضَّتَنِي النُّصْحَ لَكِنْ لَسْتُ أَسْمَعُهُ ",
        "arabic2":"إِنَّ الْمُحِبَّ عَنِ الْعُذَّالِ فِي صَمَمِ"
    },
    {
        "id": 12,
        "arabic": "إِنِّي اتَّهَمْتُ نَصِيْحَ الشَّيْبِ فِي عَذَلِي ",
        "arabic2":"وَالشَّيْبُ أَبْعَدُ فِي نُصْحٍ عَنِ التُّهَمِ"
    },
    
    
        {
            "id": 13,
            "arabic": "فَإِنَّ أَمَّارَتِي بِالسُّوْءِ مَا اتَّعَظَتْ ",
            "arabic2":"مِنْ جَهْلِهَا بِنَذِيْرِ الشَّيْبِ وَالْهَرَمِ"
        },
        {
            "id": 14,
            "arabic": "وَلَا أَعَدَّتْ مِنَ الْفِعْلِ الْجَمِيْلِ قِرَى ",
            "arabic2":"ضَيْفٍ أَلَمَّ بِرَأْسِي غَيْرَ مُحْتَشَمِ"
        },
        {
            "id": 15,
            "arabic": "لَوْ كُنْتُ أَعْلَمُ أَنِّي مَا أُوَقِّرُهُ ",
            "arabic2":"كَتَمْتُ سِرًّا بَدَالِي مِنْهُ بِالْكَتَمِ"
        },
        {
            "id": 16,
            "arabic": "مَنْ لِي بِرَدِّ جِمَاحٍ مِنْ غَوَايَتِهَا ",
            "arabic2":"كَمَا يُرَدُّ جِمَاحُ الْخَيْلِ بِاللُّجُمِ"
        },
        {
            "id": 17,
            "arabic": "فَلَا تَرُمْ بِالْمَعَاصِي كَسْرَ شَهْوَتِهَا",
            "arabic2":" إِنَّ الطَّعَامَ يُقَوِّي شَهْوَةَ النَّهِمِ"
        },
        {
            "id": 18,
            "arabic": "وَالنَّفْسُ كَالطِّفْلِ إِنْ تُهْمِلْهُ شَبَّ عَلَى ",
            "arabic2":"حُبِّ الرَّضَاعِ وَإِنْ تَفْطِمْهُ يَنْفَطِمِ"
        },
        {
            "id": 19,
            "arabic": "فَاصْرِفْ هَوَاهَا وَحَاذِرْ أَنْ تُوَلِّيَهُ ",
            "arabic2":"إِنَّ الْهَوَى مَا تَوَلَّى يُصْمِ أَوْ يَصِمِ"
        },
        {
            "id": 20,
            "arabic": "وَرَاعِهَا وَهِيَ فِي الْأَعْمَالِ سَائِمَةٌ ",
            "arabic2":"وَإِنْ هِيَ اسْتَحْلَتِ الْمَرْعَى فَلَا تُسِمِ"
        },
        {
            "id": 21,
            "arabic": "كَمْ حَسَّنَتْ لَذَّةً لِلْمَرْءِ قَاتِلَةً ",
            "arabic2":"مِنْ حَيْثُ لَمْ يَدْرِ أَنَّ السَّمَّ فِي الدَّسَمِ"
        },
        {
            "id": 22,
            "arabic": "وَاخْشَ الدَّسَائِسَ مِنْ جُوْعٍ وَمِنْ شِبَعٍ ",
            "arabic2":"فَرُبَّ مَخْمَصَةٍ شَرُّ مِنَ التُّخَمِ"
        },
        {
            "id": 23,
            "arabic": " وَاسْتَفْرِغِ الدَّمْعَ مِنْ عَيْنٍ قَدِ امْتَلَأَتْ ",
            "arabic2":"مِنَ الْمَحَارِمِ وَالْزَمْ حِمْيَةَ النَّدَمِ"
        },
        {
            "id": 24,
            "arabic": "وَخَالِفِ النَّفْسَ وَالشَّيْطَانَ وَاعْصِهِمَا ",
            "arabic2":"وَإِنْ هُمَا مَحَّضَاكَ النُّصْحَ فَاتَّهِمِ"
        },
        {
            "id": 25,
            "arabic": "وَلَا تُطِعْ مِنْهُمَا خَصْمًا وَلَا حَكَمًا ",
            "arabic2":"فَأَنْتَ تَعْرِفُ كَيْدَ الْخَصْمِ وَالْحَكَمِ"
        },
        {
            "id": 26,
            "arabic": " أَسْتَغْفِرُ اللَّهَ مِنْ قَوْلٍ بِلَا عَمَلٍ ",
            "arabic2":"لَقَدْ نَسَبْتُ بِهِ نَسْلًا لِذِي عُقُمِ"
        },
        {
            "id": 27,
            "arabic": "أَمَرْتُكَ الْخَيْرَ لَكِنْ مَا ائْتَمَرْتُ بِهِ ",
            "arabic2":"وَمَا اسْتَقَمْتُ فَمَا قَوْلِي لَكَ اسْتَقِمِ"
        },
        {
            "id": 28,
            "arabic": " وَلَا تَزَوَّدْتُ قَبْلَ الْمَوْتِ نَافِلَةً ",
            "arabic2":"وَلَمْ أُصَلِّ سِوَى فَرْضٍ وَلَمْ أُصَمِ"
        },
        {
            "id": 29,
            "arabic": "ظَلَمْتُ سُنَّةَ مَنْ أَحْيَا الظَّلَامَ إِلَى ",
            "arabic2":"أَنِ اشْتَكَتْ قَدَمَاهُ الضُّرَّ مِنْ وَرَمِ"
        },
        {
            "id": 30,
            "arabic": "وَشَدَّ مِنْ سَغَبٍ أَحْشَاءَهُ وَطَوَى ",
            "arabic2":"تَحْتَ الْحِجَارَةِ كَشْحًا مُتْرَفَ الْأَدَمِ"
        },
        {
            "id": 31,
            "arabic": " وَرَاوَدَتْهُ الْجِبَالُ الشُّمُّ مِنْ ذَهَبٍ ",
            "arabic2":"عَنْ نَفْسِهِ فَأَرَاهَا أَيَّمَا شَمَمِ"
        },
        {
            "id": 32,
            "arabic": "وَأَكَّدَتْ زُهْدَهُ فِيْهَا ضَرُوْرَتُهُ ",
            "arabic2":"إِنَّ الضَّرُوْرَةَ لَا تَعْدُو عَلَى الْعِصَمِ"
        },
        {
            "id": 33,
            "arabic": "وَكَيْفَ تَدْعُو إِلَى الدُّنْيَا ضَرُوْرَةُ مَنْ ",
            "arabic2":"لَوْلَاهُ لَمْ تُخْرَجِ الدُّنْيَا مِنَ الْعَدَمِ"
        },
        {
            "id": 34,
            "arabic": "مُحَمَّدٌ سَيِّدُ الْكَوْنَيْنِ وَالثَّقَلَيـ ",
            "arabic2":"نِ وَالْفَرِيْقَيْنِ مِنْ عُرْبٍ وَمِنْ عَجَمِ"
        },
        {
            "id": 35,
            "arabic": "نَبِيُّنَا الْآمِرُ النَّاهِي فَلَا أَحَدٌ ",
            "arabic2":"أَبَرَّ فِي قَوْلِ لَا مِنْهُ وَلَا نَعَمِ"
        },
        {
            "id": 36,
            "arabic": " هُوَ الْحَبِيْبُ الَّذِي تُرْجَي شَفَاعَتُهُ",
            "arabic2":" لِكُلِّ هَوْلٍ مِنَ الْأَهْوَالِ مُقْتَحِمِ"
        },
        {
            "id": 37,
            "arabic": "دَعَا إِلَى اللَّهِ فَالْمُسْتَمْسِكُوْنَ بِهَ ",
            "arabic2":"مُسْتَمْسِكُوْنَ بِحَبْلٍ غَيْرِ مُنْفَصِمِ"
        },
        {
            "id": 38,
            "arabic": "فَاقَ النَّبِيِّيْنَ فِي خَلْقٍ وَفِي خُلُقٍ ",
            "arabic2":"وَلَمْ يُدَانُوْهُ فِي عِلْمٍ وَلَا كَرَمِ"
        },
        {
            "id": 39,
            "arabic": "وَكُلُّهُمْ مِنْ رَسُوْلِ اللَّهِ مُلْتَمِسٌ ",
            "arabic2":"غَرْفًا مِنَ الْبَحْرِ أَوْ رَشْفًا مِنَ الدِّيَمِ"
        },
        {
            "id": 40,
            "arabic": "وَوَاقِفُوْنَ لَدَيْهِ عِنْدَ حَدِّهِمِ ",
            "arabic2":"مِنْ نُقْطَةِ الْعِلْمِ أَوْ مِنْ شَكْلَةِ الْحِكَمِ"
        },
        {
            "id": 41,
            "arabic": "فَهُوَ الَّذِي تَمَّ مَعْنَاهُ وَصُوْرَتُهُ ",
            "arabic2":"ثُمَّ اصْطَفَاهُ حَبِيْبًا بَارِئُ النَّسَمِ"
        },
        {
            "id": 42,
            "arabic": "مُنَزَّهٌ عَنْ شَرِيْكٍ فِي مَحَاسِنِهِ ",
            "arabic2":"فَجَوْهَرُ الْحُسْنِ فِيْهِ غَيْرُ مُنْقَسِمِ"
        },
        {
            "id": 43,
            "arabic": "دَعْ مَا ادَّعَتْهُ النَّصَارَى فِي نَبِيِّهِمِ ",
            "arabic2":"وَاحْكُمْ بِمَا شِئْتَ مَدْحًا فِيْهِ وَاحْتَكِمِ"
        },
        {
            "id": 44,
            "arabic": "وَانْسُبْ إِلَى ذَاتِهِ مَا شِئْتَ مِنْ شَرَفٍ ",
            "arabic2":"وَانْسُبْ إِلَى قَدْرِهِ مَا شِئْتَ مِنْ عِظَمِ"
        },
        {
            "id": 45,
            "arabic": "فَإِنَّ فَضْلَ رَسُوْلِ اللَّهِ لَيْسَ لَهُ ",
            "arabic2":"حَدٌّ فَيُعْرِبُ عَنْهُ نَاطِقٌ بِفَمِ"
        },
        {
            "id": 46,
            "arabic": "لَوْ نَاسَبَتْ قَدْرَهُ آيَاتُهُ عِظَمًا ",
            "arabic2":"أَحْيَا اسْمُهُ حِيْنَ يُدْعَى دَارِسَ الرِّمَمِ"
        },
        {
            "id": 47,
            "arabic": "لَمْ يَمْتَحِنَّا بِمَا تَعْيَا الْعُقُوْلُ بِهِ ",
            "arabic2":"حِرْصًا عَلَيْنَا فَلَمْ نَرْتَبْ وَلَمْ نَهِمِ"
        },
        {
            "id": 48,
            "arabic": "أَعْيَا الْوَرَى فَهْمُ مَعْنَاهُ فَلَيْسَ يُرَى ",
            "arabic2":"فِي الْقُرْبِ وَالْبُعْدِ فِيْهِ غَيْرُ مُنْفَحِمِ"
        },
        {
            "id": 49,
            "arabic": "كَالشَّمْسِ تَظْهَرُ لِلْعَيْنَيْنِ مِنْ بُعُدٍ ",
            "arabic2":"صَغِيْرَةً وَتُكِلُّ الطَّرْفَ مِنْ أَمَمِ"
        },
        {
            "id": 50,
            "arabic": "وَكَيْفَ يُدْرِكُ فِي الدُّنْيَا حَقِيْقَتَهُ ",
            "arabic2":"قَوْمٌ نِيَامٌ تَسَلَّوْا عَنْهُ بِالْحُلُمِ"
        },
        {
            "id": 51,
            "arabic": "فَمَبْلَغُ الْعِلْمِ فِيْهِ أَنَّهُ بَشَرٌ ",
            "arabic2":"وَأَنَّهُ خَيْرُ خَلْقِ اللَّهِ كُلِّهِمِ"
        },
        {
            "id": 52,
            "arabic": "وَكُلُّ آيٍ أَتَى الرُّسْلُ الْكِرَامُ بِهَا ",
            "arabic2":"فَإِنَّمَا اتَّصَلَتْ مِنْ نُوْرِهِ بِهِمِ"
        },
        {
            "id": 53,
            "arabic": "فَإِنَّهُ شَمْسٌ فَضْلٍ هُمْ كَوَاكِبُهَا ",
            "arabic2":"يُظْهِرْنَ أَنْوَارَهَا لِلنَّاسِ فِي الظُّلَمِ"
        },
        {
            "id": 54,
            "arabic": "أَكْرِمْ بِخَلْقٍ نَبِيٍّ زَانَهُ خُلُقٌ ",
            "arabic2":"بِالْحُسْنِ مُشْتَمِلٍ بِالْبِشْرِ مُتَّسِمِ"
        },
        {
            "id": 55,
            "arabic": "كَالزَّهْرِ فِي تَرَفٍ وَالْبَذْرِ فِي شَرَفٍ ",
            "arabic2":"وَالْبَحْرِ فِي كَرَمٍ وَالدَّهْرِ فِي هِمَمِ"
        },
        {
            "id": 56,
            "arabic": "كَأَنَّهُ وَهُوَ فَرْدٌ مِنْ جَلَالَتِهِ ",
            "arabic2":"فِي عَسْكِرٍ حِيْنَ تَلْقَاهُ وَفِي حَشَمِ"
        },
        {
            "id": 57,
            "arabic": "كَأَنَّمَا اللُّؤْلُؤُ الْمَكْنُوْنُ فِي صَدَفٍ ",
            "arabic2":"مِنْ مَعْدِنَيْ مَنْطِقٍ مِنْهُ وَمُبْتَسَمِ"
        },
        {
            "id": 58,
            "arabic": " لَا طِيْبَ يَعْدِلُ تُرْبًا ضَمَّ أَعْظُمَهُ ",
            "arabic2":"طُوْبَى لِمُنْتَشِقٍ مِنْهُ وَمُلْتَثِمِ"
        },
        {
            "id": 59,
            "arabic": "أَبَانَ مَوْلِدُهُ عَنْ طِيْبِ عُنْصُرِهِ ",
            "arabic2":"يَا طِيْبَ مُبْتَدَإٍ مِنْهُ وَمُخْتَتَمِ"
        },
        {
            "id": 60,
            "arabic": "يَوْمٌ تَفَرَّسَ فِيْهِ الْفُرْسُ أَنَّهُمُ ",
            "arabic2":"قَدْ أُنْذِرُوا بِحُلُوْلِ الْبُؤْسِ وَالنِّقَمِ"
        },
        {
            "id": 61,
            "arabic": "وَبَاتَ إِيْوَانُ كِسْرَى وَهُوَ مُنْصَدِعٌ ",
            "arabic2":"كَشَمْلِ أَصْحَابِ كِسْرَى غَيْرَ مُلْتَئِمِ"
        },
        {
            "id": 62,
            "arabic": "وَالنَّارُ خَامِدَةُ الْأَنْفَاسِ مِنْ أَسَفٍ ",
            "arabic2":"عَلَيْهِ وَالنَّهْرُ سَاهِي الْعَيْنِ مِنْ سَدَمِ"
        },
        {
            "id": 63,
            "arabic": "وَسَاءَ سَاوَةَ أَنْ غَاضَتْ بُحَيْرَتُهَا ",
            "arabic2":"وَرُدَّ وَارِدُهَا بِالْغَيْظِ حِيْنَ ظَمِي"
        },
        {
            "id": 64,
            "arabic": "كَأَنَّ بِالنَّارِ مَا بِالْمَاءِ مِنْ بَلَلٍ ",
            "arabic2":"حُزْنًا وَبِالْمَاءِ مَا بِالنَّارِ مِنْ ضَرَمِ"
        },
        {
            "id": 65,
            "arabic": " وَالْجِنُّ تَهْتِفُ وَالْأَنْوَارُ سَاطِعَةٌ ",
            "arabic2":"وَالْحَقُّ يَظْهَرُ مِنْ مَعْنًى وَمِنْ كَلِمِ"
        },
        {
            "id": 66,
            "arabic": "عَمُوا وَصَمُّوا فَإِعْلَانُ الْبَشَائِرِ لَمْ ",
            "arabic2":"تُسْمَعْ وَبَارِقَةُ الْإِنْذَارِ لَمْ تُشَمِ"
        },
        {
            "id": 67,
            "arabic": "مِنْ بَعْدِ مَا أَخْبَرَ الْأَقْوَامَ كَاهِنُهُمْ ",
            "arabic2":"بِأَنَّ دِيْنَهُمُ الْمُعْوَجَّ لَمْ يَقُمِ"
        },
        {
            "id": 68,
            "arabic": " وَبَعْدَمَا عَايَنُوا فِي الْأُفْقِ مِنْ شُهُبٍ ",
            "arabic2":"مُنْقَضَّةٍ وِفْقَ مَا فِي الْأَرْضِ مِنْ صَنَمِ"
        },
        {
            "id": 69,
            "arabic": "حَتَّى غَدَا عَنْ طَرِيْقِ الْوَحْيِ مُنْهَزِمٌ ",
            "arabic2":"مِنَ الشَّيَاطِيْنِ يَقْفُوا إِثْرَ مُنْهَزِمِ"
        },
        {
            "id": 70,
            "arabic": "كَأَنَّهُمْ هَرَبًا أَبْطَالُ أَبْرَهَةٍ ",
            "arabic2":"أَوْ عَسْكَرٌ بِالْحَصَى مِنْ رَاحَتَيْهِ رُمِي"
        },
        {
            "id": 71,
            "arabic": " نَبْذًا بِهِ بَعْدَ تَسْبِيْحٍ بِبَطْنِهِمَا ",
            "arabic2":"نَبْذَ الْمُسَبِّحِ مِنْ أَحْشَاءِ مُلْتَقِمِ"
        },
        {
            "id": 72,
            "arabic": "جَاءَتْ لِدَعْوِتِه الْأَشْجَارُ سَاجِدَةً ",
            "arabic2":"تَمْشِي إِلَيْهِ عَلَى سَاقٍ بِلَا قَدَمِ"
        },
        {
            "id": 73,
            "arabic": "كَأَنَّمَا سَطَرَتْ سَطْرًا لِمَا كَتَبَتْ ",
            "arabic2":"فُرُوْعُهَا مِنْ بَدِيْعِ الْخَطِّ بِاللَّقَمِ"
        },
        {
            "id": 74,
            "arabic": " مِثْلَ الْغَمَامَةِ أَنَّى سَارَ سَائِرَةً ",
            "arabic2":"تَقِيْهِ حَرَّ وَطِيْسٍ لِلْهَجِيْرِ حَمِي"
        },
        {
            "id": 75,
            "arabic": "أَقْسَمْتُ بِالْقَمَرِ الْمُنْشَقِّ إِنَّ لَهُ ",
            "arabic2":"مِنْ قَلْبِهِ نِسْبَةً مَبْرُوْرَةَ الْقَسَمِ"
        },
        {
            "id": 76,
            "arabic": "وَمَا حَوَى الْغَارُ مِنْ خَيْرٍ وَمِنْ كَرَمِ ",
            "arabic2":"وَكُلُّ طَرْفٍ مِنَ الْكُفَّارِ عَنْهُ عَمِي"
        },
        {
            "id": 77,
            "arabic": "فَالصِّدْقُ فِي الْغَارِ وَالصِّدِّيْقُ لَمْ يَرِمَا",
            "arabic2":" وَهُمْ يَقُوْلُوْنَ مَا بِالْغَارِ مِنْ أَرِمِ"
        },
        {
            "id": 78,
            "arabic": "ظَنُّوا الْحَمَامَ وَظَنُّوا الْعَنْكَبُوْتَ عَلَى ",
            "arabic2":"خَيْرِ الْبَرِيَّةِ لَمْ تَنْسُجْ وَلَمْ تَحُمِ"
        },
        {
            "id": 79,
            "arabic": "وِقَايَةُ اللَّهِ أَغْنَتْ عَنْ مُضَاعَفَةٍ  ",
            "arabic2":"مِنَ الدُّرُوْعِ وَعَنْ عَالٍ مِنَ الْأُطُمِ"
        },
        {
            "id": 80,
            "arabic": "مَا سَامَنِي الدَّهْرُ ضَيْمًا وَاسْتَجَرْتُ بِهِ",
            "arabic2":" إِلَّا وَنِلْتَ جِوَارًا مِنْهُ لَمْ يُضَمِ"
        },
        {
            "id": 81,
            "arabic": "وَلَا الْتَمَسْتُ غِنَى الدَّارَيْنِ مِنْ يَدِهِ ",
            "arabic2":"إِلَّا اسْتَلَمْتُ النَّدَى مِنْ خَيْرِ مُسْتَلَمِ"
        },
        {
            "id": 82,
            "arabic": "لَا تُنْكِرِ الْوَحْيَ مِنْ رُؤْيَاهُ إِنَّ لَهُ ",
            "arabic2":"قَلْبًا إِذَا نَامَتِ الْعَيْنَانِ لَمْ يَنَمِ"
        },
        {
            "id": 83,
            "arabic": "وَذَاكَ حِيْنَ بُلُوْغٍ مِنْ نُبُوَّتِهِ ",
            "arabic2":"فَلَيْسَ يُنْكِرُ فِيْهِ حَالُ مُحْتَلِمِ"
        },
        {
            "id": 84,
            "arabic": " تَبَارَكَ اللَّهُ مَا وَحْيٌ بِمُكْتَسِبٍ ",
            "arabic2":"وَلَا نَبِيٌّ عَلَى غَيْبٍ بِمُتَّهِمِ"
        },
        {
            "id": 85,
            "arabic": "كَمْ أَبْرَأَتْ وَصِبًا بِاللَّمْسِ رَاحَتُهُ",
            "arabic2":" وَأَطْلَقَتْ أَرِبًا مِنْ رِبْقَهِ اللَّمَمِ"
        },
        {
            "id": 86,
            "arabic": "وَأَحْيَتِ السَّنَةَ الشَّهْبَاءَ دَعْوَتُهُ ",
            "arabic2":"حَتَّى حَكَتْ غُرَّةً فِي الْأَعْصُرِ الدُّهُمِ"
        },
        {
            "id": 87,
            "arabic": "بِعَارِضٍ جَادَ أَوْ خِلْتَ الْبِطَاحَ بِهَا ",
            "arabic2":"سَيْبًا مِنَ الْيَمِّ أَوْ سَيْلًا مِنَ الْعَرِمِ"
        },
        {
            "id": 88,
            "arabic": "دَعْنِي وَوَصْفِيَ آيَاتٍ لَهُ ظَهَرَتْ ",
            "arabic2":"ظُهُوْرَ نَارِ الْقِرَى لَيْلًا عَلَى عَلَمِ"
        },
        {
            "id": 89,
            "arabic": "فَالدُّرُّ يَزْدَادُ حُسْنًا وَهُوَ مُنْتَظِمٌ ",
            "arabic2":"وَلَيْسَ يَنْقُصُ قَدْرًا غَيْرَ مُنْتَظِمِ"
        },
        {
            "id": 90,
            "arabic": "فَمَا تَطَاوُلُ آمَالِ الْمَدِيْحِ إِلَى ",
            "arabic2":"مَا فِيْهِ مِنْ كَرَمِ الْأَخْلَاقِ وَالشِّيَمِ"
        },
        {
            "id": 91,
            "arabic": "آيَاتُ حَقٍّ مِنَ الرَّحْمَنِ مُحْدَثَةٌ ",
            "arabic2":" قَدِيْمَةٌ صِفَةُ الْمَوْصُوْفِ بِالْقِدَمِ"
        },
        {
            "id": 92,
            "arabic": "لَمْ تَقْتَرِنْ بِزَمَانٍ وَهِيَ تُخْبِرُنَا ",
            "arabic2":"عَنِ الْمَعَادِ وَعَنْ عَاٍد وَعَنْ إِرَمِ"
        },
        {
            "id": 93,
            "arabic": " دَامَتْ لَدَيْنَا فَفَاقَتْ كُلَّ مُعْجِزَةً ",
            "arabic2":"مِنَ النَّبِيِّيْنَ إِذْ جَاءَتْ وَلَمْ تَدُمِ"
        },
        {
            "id": 94,
            "arabic": "مُحَكَّمَاتٌ فَمَا تُبْقِيْنَ مِنْ شُبَهٍ ",
            "arabic2":"لِذِي شِقَاقٍ وَمَا تَبْغِيْنَ مِنْ حَكَمِ"
        },
        {
            "id": 95,
            "arabic": "مَا حُوْرِبَتْ قَطُّ إِلَّا عَادَ مِنْ حَرَبٍ ",
            
            "arabic2":"أَعْدَى الْأَعَادِي إِلَيْهَا مُلْقِيَ السَّلَمِ"
        },
        {
            "id": 96,
            "arabic": " رَدَّتْ بَلَاغَتُهَا دَعْوَى مُعَارِضِهَا ",
            "arabic2":"رَدَّ الْغَيُوْرِ يَدَ الْجَانِي عَنِ لْحَرَمِ"
        },
        {
            "id": 97,
            "arabic": "لَهَا مَعَانٍ كَمَوْجِ الْبَحْرِ فِي مَدَدٍ ",
            "arabic2":"وَفَوْقَ جَوْهَرِهِ فِي الْحُسْنِ وَالْقِيَمِ"
        },
        {
            "id": 98,
            "arabic": "فَمَا تُعَدُّ وَلَا تُحْصَى عَجَائِبُهَا ",
            "arabic2":"وَلَا تُسَامُ عَلَى الْإِكْثَارِ بَالسَّأَمِ"
        },
        {
            "id": 99,
            "arabic": "قَرَّتْ بِهَا عَيْنُ قَارِيْهَا فَقُلْتُ لَهُ ",
            "arabic2":"لَقَدْ ظَفِرْتَ بِحَبْلِ اللَّهِ فَاعْتَصِمِ"
        },
        {
            "id": 100,
            "arabic": "إِنْ تَتْلُهَا خِيْفَةً مِنْ حَرِّ نَارِ لَظَى ",
            "arabic2":"أَطْفَأْتَ حَرَّ لَظَى مِنْ وِرْدِهَا الشِّيَمِ"
        },
        {
            "id": 101,
            "arabic": "كَأَنَّهَا الْحَوْضُ تَبْيَضُّ الْوُجُوْهُ بِهِ ",
            "arabic2":"مِنَ الْعُصَاةِ وَقَدْ جَائُوْهُ كَالْحُمَمِ"
        },
        {
            "id": 102,
            "arabic": "وَكَالصِّرَاطِ وَكَالْمِيْزَانِ مَعْدِلَةً ",
            "arabic2":"فَالْقِسْطُ مِنْ غَيْرِهَا فِي النَّاسِ لَمْ يَقُمِ"
        },
        {
            "id": 103,
            "arabic": " لَا تَعْجَبَنْ لِحَسُوْدٍ رَاحَ يُنْكِرُهَا ",
            "arabic2":"تَجَاهُلًا وَهُوَ عَيْنُ الْحَاذِقِ الْفَهِمِ"
        },
        {
            "id": 104,
            "arabic": " قَدْ تُنْكِرُ الْعَيْنُ ضَوْءَ الشَّمْسِ مِنْ رَمَدٍ",
            "arabic2":" وَيُنْكِرُ الْفَمُ طَعْمَ الْمَاءِ مِنْ سَقَمِ"
        },
        {
            "id": 105,
            "arabic": "يَا خَيْرَ مَنْ يَمَّمَ الْعَافُوْنَ سَاحَتَهُ ",
            "arabic2":"سَعْيًا وَفَوْقَ مُتُوْنِ الْأَيْنُقِ الرُّسُمِ"
        },
        {
            "id": 106,
            "arabic": "وَمَنْ هُوَ الْآيَةُ الْكُبْرَى لِمُعْتَبِرٍ ",
            "arabic2":"وَمَنْ هُوَ النِّعْمَةُ الْعُظْمَى لِمُغْتَنِمِ"
        },
        {
            "id": 107,
            "arabic": " سَرَيْتَ مِنْ حَرَمٍ لَيْلًا إِلَى حَرَمٍ ",
            "arabic2":"كَمَا سَرَى الْبَدْرُ فِي دَاجٍ مِنَ الظُّلَمِ"
        },
        {
            "id": 108,
            "arabic": "وَبِتَّ تَرْقَى إِلَى أَنْ نِلْتَ مَنْزِلَةً ",
            "arabic2":"مِنْ قَابِ قَوْسَيْنِ لَمْ تُدْرَكْ وَلَمْ تُرَمِ"
        },
        {
            "id": 109,
            "arabic": "وَقَدَّمَتْكَ جَمِيْعُ الْأَنْبِيَاءِ بِهَا ",
            "arabic2":"وَالرُّسْلِ تَقْدِيْمَ مَخْدُوْمٍ عَلَى خَدَمِ"
        },
        {
            "id": 110,
            "arabic": "وَأَنْتَ تَخْتَرِقُ السَّبْعَ الطِّبَاقَ بِهِمْ ",
            "arabic2":"فِي مَوْكِبٍ كُنْتَ فِيْهِ الصَّاحِبَ الْعَلَمِ"
        },
        {
            "id": 111,
            "arabic": "حَتَّى إِذَا لَمْ تَدَعْ شَأْوًا لِمُسْتَبِقٍ ",
            "arabic2":"مِنَ الدُّنُوِّ وَلَا مَرْقَى لِمُسْتَنِمِ"
        },
        {
            "id": 112,
            "arabic": "خَفَضْتَ كُلَّ مَقَامٍ بِالْإِضَافَةِ إِذْ ",
            "arabic2":"نُوْدِيْتَ بِالرَّفْعِ مِثْلَ الْمُفْرَدِ الْعَلَمِ"
        },
        {
            "id": 113,
            "arabic": "كَيْمَا تَفُوْزَ بِوَصْلٍ أَيِّ مُسْتَتِرٍ",
            "arabic2":"عَنِ الْعُيُوْنِ وَسِرٍّ أَيِّ مُكْتَتَمِ"
        },
        {
            "id": 114,
            "arabic": "فَحُزْتَ كُلَّ فَخَارٍ غَيْرَ مُشْتَرَكٍ ",
            "arabic2":"وَجُزْتَ كُلَّ مَقَامٍ غَيْرَ مُزْدَحَمِ"
        },
        {
            "id": 115,
            "arabic": "وَجَلَّ مِقْدَارُ مَا وُلِّيْتَ مِنْ رُتَبٍ ",
            "arabic2":"وَعَزَّ إِدْرَاكُ مَا أُوْلِيْتَ مِنْ نِعَمِ"
        },
        {
            "id": 116,
            "arabic": "بُشْرَى لَنَا مَعْشَرَ الْإِسْلَامِ إِنَّ لَنَا ",
            "arabic2":"مِنَ الْعِنَايَةِ رُكْنًا غَيْرَ مُنْهَدِمِ"
        },
        {
            "id": 117,
            "arabic": "لَمَّا دَعَا اللَّهُ دَاعِيْنَا لِطَاعَتِهِ ",
            "arabic2":"بِأَكْرَمِ الرُّسْلِ كُنَّا أَكْرَمَ الْأُمَمِ"
        },
        {
            "id": 118,
            "arabic": "رَاعَتْ قُلُوْبَ الْعِدَا أَنْبَاءُ بِعْثَتِهِ ",
            "arabic2":"كَنَبْأَةٍ أَجْلَفَتْ غُفْلًا مِنَ الْغَنَمِ"
        },
        {
            "id": 119,
            "arabic": "مَا زَالَ يَلْقَاهُمُ فِي كُلِّ مُعْتَرَكٍ",
            "arabic2":" حَتَّى حَكَوْا بِالْقَنَا لَحْمًا عَلَى وَضَمِ"
        },
        {
            "id": 120,
            "arabic": "وَدُّوا الْفِرَارَ فَكَانُوا يَغْبِطُوْنَ بِهِ ",
            "arabic2":"أَشْلَاءَ شَالَتْ مَعَ الْعِقْبَانِ وَالرَّخَمِ"
        },
        {
            "id": 121,
            "arabic": "تَمْضِي اللَّيَالِيَ وَلَا يَدْرُوْنَ عِدَّتَهَا ",
            "arabic2":"مَا لَمْ تَكُنْ مِنْ لَيَالِي الْأَشْهُرِ الْحُرُمِ"
        },
        {
            "id": 122,
            "arabic": "كَأَنَّمَا الدِّيْنُ ضَيْفٌ حَلَّ سَاحَتَهُمْ ",
            "arabic2":"بِكُلِّ قَرْمٍ إِلَى لَحْمِ الْعِدَا قَرَمِ"
        },
        {
            "id": 123,
            "arabic": "يَجُرُّ بَحْرَ خَمِيْسٍ فَوْقَ سَابِحَةٍ ",
            "arabic2":"يَرْمِي بِمَوْجٍ مِنَ الْأَبْطَالِ مُلْتَطِمِ"
        },
        {
            "id": 124,
            "arabic": "مِنْ كُلِّ مُنْتَدِبٍ لِلَّهِ مُحْتَسِبٍ ",
            "arabic2":"يَسْطُو بِمُسْتَأْصِلٍ لِلْكُفْرِ مُصْطَلِمِ"
        },
        {
            "id": 125,
            "arabic": "حَتَّى غَدَتْ مِلَّةُ الْإِسْلَامِ وَهِيَ بِهِمْ ",
            "arabic2":"مِنْ بَعْدِ غُرْبَتِهَا مَوْصُوْلَةَ الرَّحِمِ"
        },
        {
            "id": 126,
            "arabic": "مَكْفُوْلَةً أَبَدًا مِنْهُمْ بِخَيْرِ أَبٍ ",
            "arabic2":"وَخَيْرِ بَعْلٍ فَلَمْ تَيْتَمْ وَلَمْ تَئِمِ"
        },
        {
            "id": 127,
            "arabic": "هُمُ الْجِبَالُ فَسَلْ عَنْهُمْ مُصَادِمَهُمْ ",
            "arabic2":"مَاذَا رَأَى مِنْهُمُ فِي كُلِّ مُصْطَدَمِ"
        },
        {
            "id": 128,
            "arabic": " وَسَلْ حُنَيْنًا وَسَلْ بَدْرًا وَسَلْ أُحُدًا",
            "arabic2":" فُصُوْلَ حَتْفٍ لَهُمْ أَدْهَى مِنَ الْوَخَمِ"
        },
        {
            "id": 129,
            "arabic": "الْمُصْدِرِي الْبِيْضِ حُمْرًا بَعْدَ مَا وَرَدَتْ",
            "arabic2":" مِنَ الْعِدَا كُلَّ مُسْوَدٍّ مِنَ اللِّمَمِ"
        },
        {
            "id": 130,
            "arabic": "وَالْكَاتِبِيْنَ بِسُمْرِ الْخَطِّ مَا تَرَكَتْ ",
            "arabic2":"أَقْلَامُهُمْ حَرْفَ جِسْمٍ غَيْرَ مُنْعَجِمِ"
        },
        {
            "id": 131,
            "arabic": "شَاكِي السِّلَاحِ لَهُمْ سِيْمَا تُمَيِّزُهُمْ ",
            "arabic2":"وَالْوَرْدُ يَمْتَازُ بِالسِّيْمَا عَنِ السَّلَمِ"
        },
        {
            "id": 132,
            "arabic": "تُهْدِي إِلَيْكَ رِيَاحُ النَّصْرِ نَشْرَهُمُ ",
            "arabic2":"فَتَحْسَبُ الزَّهْرَ فِي الْأَكْمَامِ كُلَّ كَمِي"
        },
        {
            "id": 133,
            "arabic": "كَأَنَّهُمْ فِي ظُهُوْرِ الْخَيْلِ نَبْتُ رُبًا ",
            "arabic2":"مِنْ شِدَّةِ الْحَزْمِ لَا مِنْ شِدَّةِ الْحُزُمِ"
        },
        {
            "id": 134,
            "arabic": "طَارَتْ قُلُوْبَ الْعِدَا مِنْ بَأْسِهِمْ فَرَقًا ",
            "arabic2":"فَمَا تُفَرِّقُ بَيْنَ الْبَهْمِ وَالْبُهَمِ"
        },
        {
            "id": 135,
            "arabic": "وَمَنْ تَكُنْ بِرَسُوْلِ اللَّهِ نُصْرَتُهُ ",
            "arabic2":"إِنْ تَلْقَهُ الْأُسْدُ فِي أَجَامِهَا تَجِمَ"
        },
        {
            "id": 136,
            "arabic": "وَلَنْ تَرَى مِنْ وَلِيٍّ غَيْرِ مُنْتَصِرٍ ",
            "arabic2":"بِهِ وَلَا مِنْ عَدُوٍّ غَيْرَ مُنْقَصِمِ"
        },
        {
            "id": 137,
            "arabic": " أَحَلَّ أُمَّتَهُ فِي حِرْزِ مِلَّتِهِ ",
            "arabic2":"كَاللَّيْثِ حَلَّ مَعَ الْأَشْبَالِ فِي أَجَمِ"
        },
        {
            "id": 138,
            "arabic": "كَمْ جَدَّلَتْ كَلِمَاتُ اللَّهِ مِنْ جَدَلٍ ",
            "arabic2":" فِيْهِ وَكَمْ خَصَمَ الْبُرْهَانُ مِنْ خَصِمِ"
        },
        {
            "id": 139,
            "arabic": "كَفَاكَ بِالْعِلْمِ فِي الْأُمِّيِّ مُعْجِزَةً ",
            "arabic2":"فِي الْجَاهِلِيَّةِ وَالتَّأْدِيْبِ فِي الْيُتُمِ"
        },
        {
            "id": 140,
            "arabic": "خَدَمْتُهُ بِمَدِيْحٍ أَسْتَقِيْلُ بِهِ ",
            "arabic2":"ذُنُوْبَ عُمْرٍ مَضَى فِي الشِّعْرِ وَالْخِدَمِ"
        },
        {
            "id": 141,
            "arabic": "إِذْ قَلَّدَانِيَ مَا تُخْشَى عَوَاقِبُهُ ",
            "arabic2":"كَأَنَّنِي بِهِمَا هَدْيٌ مِنَ النَّعَمِ"
        },
        {
            "id": 142,
            "arabic": "أَطَعْتُ غَيَّ الصِّبَا فِي الْحَالَتَيْنِ وَمَا",
            "arabic2":" حَصَلْتُ إِلَّا عَلَى الْأَثَامِ وَالنَّدَمِ"
        },
        {
            "id": 143,
            "arabic": "فَيَا خَسَارَةَ نَفْسٍ فِي تِجَارَتِهَا ",
            "arabic2":"لَمْ تَشْتَرِ الدِّيْنُ بِالدُّنْيَا وَلَمْ تُسَمِ"
        },
        {
            "id": 144,
            "arabic": "وَمَنْ يَبِعْ آجِلًا مِنْهُ بِعَاجِلِهِ ",
            "arabic2":"يَبِنْ لَهُ الْغَبْنُ فِي بَيْعٍ وَفِي سَلَمِ"
        },
        {
            "id": 145,
            "arabic": "إِنْ آتِ ذَنْبًا فَمَا عَهْدِي بِمُنْتَقِضٍ",
            "arabic2":" مِنَ النَّبِيِّ وَلَا حَبْلِي بِمُنْصَرِمِ"
        },
        {
            "id": 146,
            "arabic": "فَإِنَّ لِي ذِمَّةً مِنْهُ بِتَسْمِيَتِي ",
            "arabic2":"مُحَمَّدًا وَهُوَ أَوْفَى الْخَلْقِ بِالذِّمَمِ"
        },
        {
            "id": 147,
            "arabic": "إِنْ لَمْ يَكُنْ فِي مَعَادِي آخِذًا بِيَدِي ",
            "arabic2":"فَضْلًا وَإِلَّا فَقُلْ يَا زَلَّةَ الْقَدَمِ"
        },
        {
            "id": 148,
            "arabic": "حَاشَاهُ أَنْ يُحْرِمَ الرَّاجِيْ مَكَارِمَهُ ",
            "arabic2":"أَوْ يَرْجِعَ الْجَارُ مِنْهُ غَيْرُ مُحْتَرَمِ"
        },
        {
            "id": 149,
            "arabic": "وَمُنْذُ أَلْزَمْتُ أَفْكَارِي مَدَائِحَهُ ",
            "arabic2":"وَجَدْتُهُ لِخَلَاصِي خَيْرَ مُلْتَزِمِ"
        },
        {
            "id": 150,
            "arabic": "وَلَنْ يَفُوْتَ الْغِنَى مِنْهُ يَدًا تَرِبَتْ",
            "arabic2":" إِنَّ الْحَيَا يُنْبِتُ الْأَزْهَارَ فِي الْأَكَمِ"
        },
        {
            "id": 151,
            "arabic": "وَلَمْ أُرِدْ زَهْرَةَ الدُّنْيَا الَّتِي اقْتَطَفَتْ ",
            "arabic2":"يَدَا زُهَيْرٍ بِمَا أَثْنَى عَلَى هَرَمِ"
        },
        {
            "id": 152,
            "arabic": "يَا أَكْرَم َالْخَلْقِ مَالِي مَنْ أَلُوْذُ بِهِ",
            "arabic2":" سِوَاكَ عِنْدَ حُلُوْلِ الْحَادِثِ الْعَمَمِ"
        },
        {
            "id": 153,
            "arabic": "وَلَنْ يَضِيْقَ رَسُوْلَ اللَّهِ جَاهُكَ بِي ",
            "arabic2":"إِذَا الْكَرِيْمُ تَجَلَّى بِاسْمِ مُنْتَقِمِ"
        },
        {
            "id": 154,
            "arabic": "فَإِنَّ مِنْ جُوْدِكَ الدُّنْيَا وَضَرَّتَهَا ",
            "arabic2":"وَمِنْ عُلُوْمِكَ عِلْمَ اللَّوْحِ وَالْقَلَمِ"
        },
        {
            "id": 155,
            "arabic": " يَا نَفْسُ لَا تَقْنَطِي مِنْ زَلَّةٍ عَظُمَتْ ",
            "arabic2":"إِنَّ الْكَبَائِرَ فِي الْغُفْرَانِ كَاللَّمَمِ"
        },
        {
            "id": 156,
            "arabic": "لَعَلَّ رَحْمَةَ رَبِّي حِيْنَ يَقْسِمُهَا ",
            "arabic2":"تَأْتِي عَلَى حَسَبِ الْعِصْيَانِ فِي الْقِسَمِ"
        },
        {
            "id": 157,
            "arabic": "يَا رَبِّ وَاجْعَلْ رَجَائِي غَيْرَ مُنْعَكِسٍ ",
            "arabic2":"لَدَيْكَ وَاجْعَلْ حِسَابِي غَيْرَ مُنْخَزِمِ"
        },
        {
            "id": 158,
            "arabic": " وَالْطُفْ بِعَبْدِكَ فِي الدَّارَيْنِ إِنَّ لَهُ",
            "arabic2":" صَبْرًا مَتَى تَدْعُهُ الْأَهْوَالِ يَنْهَزِمِ"
        },
        {
            "id": 159,
            "arabic": "وَأْذَنْ لِسُحْبِ صَلَاةٍ مِنْكَ دَائِمَةٍ ",
            "arabic2":"عَلَى النَّبِيِّ بِمُنْهَلٍّ وَمُنْسَجِمِ"
        },
        {
            "id": 160,
            "arabic": "وَالْآلِ وَالصَّحْبِ ثُمَّ التَّابِعِيْنَ فَهُمْ",
            "arabic2":" أَهْلُ التُّقَى وَالنَّقَا وَالْحِلْمِ وَالْكَرَمِ"
        },
        {
            "id": 161,
            "arabic": "مَا رَنَّحَتْ عَذَبَاتِ الْبَانِ رِيْحُ صَبًا ",
            "arabic2":"وَأَطْرَبَ الْعِيْسَ حَادِي الْعِيْسِ باِلنَّغَمِ"
        }
    

]